<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="content">
            <referenceBlock name="vault.cards.list">
                <block class="Unzer\PAPI\Block\Customer\CardsRenderer" name="unzer.card.renderer" template="Unzer_PAPI::customer_account/credit_card.phtml"/>
            </referenceBlock>
            <referenceBlock name="vault.token.list">
                <block class="Unzer\PAPI\Block\Customer\PaypalRenderer" name="unzer.token.renderer" template="Unzer_PAPI::customer_account/paypal.phtml"/>
            </referenceBlock>
        </referenceContainer>
    </body>
</page>
